<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boot.admin.system.modules.system.mapper.JobMapper">
    <select id="selectByUserId" resultType="com.boot.admin.system.modules.system.domain.JobDO">
        SELECT t.* FROM sys_job t
        INNER JOIN sys_users_jobs suj ON suj.job_id = t.id AND suj.user_id = #{userId}
    </select>
</mapper>