<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boot.admin.system.modules.quartz.mapper.QuartzJobMapper">
    <sql id="querySql">
        SELECT * FROM sys_quartz_job ${ew.customSqlSegment}
        ORDER BY id DESC
    </sql>

    <select id="pageQuery" resultType="com.boot.admin.system.modules.quartz.domain.QuartzJobDO">
        <include refid="querySql"/>
    </select>

    <select id="queryAll" resultType="com.boot.admin.system.modules.quartz.domain.QuartzJobDO">
        <include refid="querySql"/>
    </select>
</mapper>