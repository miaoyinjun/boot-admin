<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jjche.sys.modules.quartz.mapper.QuartzLogMapper">
    <sql id="querySql">
        SELECT * FROM sys_quartz_log ${ew.customSqlSegment}
    </sql>

    <select id="pageQuery" resultType="org.jjche.sys.modules.quartz.domain.QuartzLogDO">
        <include refid="querySql"/>
    </select>

    <select id="queryAll" resultType="org.jjche.sys.modules.quartz.domain.QuartzLogDO">
        <include refid="querySql"/>
    </select>
</mapper>
